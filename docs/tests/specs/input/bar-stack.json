{
    "$schema": "https://vega.github.io/schema/vega-lite/v4.json",
    "data": {
        "url": "../../../sample-data/titanicmaster.tsv",
        "format": {
            "type": "tsv"
        }
    },
    "height": 500,
    "width": 500,
    "mark": "bar",
    "encoding": {
        "y": {
            "field": "Gender",
            "type": "nominal"
        },
        "x": {
            "field": "Survived",
            "type": "quantitative",
            "aggregate": "count"
        },
        "color": {
            "field": "Class",
            "type": "nominal"
        }
    }
}